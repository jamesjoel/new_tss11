  <div class="row">
    <div class="col-md-12">
      <div id="carouselLink" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img class="d-block w-100" src="/imgs/sale1.jpg" alt="First slide">
          </div>
          <div class="carousel-item">
            <img class="d-block w-100" src="/imgs/sale2.jpg" alt="Second slide">
          </div>
          <div class="carousel-item">
            <img class="d-block w-100" src="/imgs/sale3..jpg" alt="Third slide">
          </div>
        </div>
        <a class="carousel-control-prev" href="#carouselLink" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselLink" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
    </div>
  </div>
  <div class="row ">
    <div class="col-md-3">
        <div class="list-group listing">
          <a href="#" class="list-group-item list-group-item-action">
            <i class="fa fa-newspaper-o" aria-hidden="true"></i> JD News
          </a>
          <a href="#" class="list-group-item list-group-item-action"><i class="fa fa-building-o" aria-hidden="true"></i> Hotels</a>
          <a href="#" class="list-group-item list-group-item-action"><i class="fa fa-plane" aria-hidden="true"></i> Air Ticket</a>
          <a href="#" class="list-group-item list-group-item-action"><i class="fa fa-money" aria-hidden="true"></i> Apply For Loan</a>
          <a href="#" class="list-group-item list-group-item-action"><i class="fa fa-car" aria-hidden="true"></i> Auto Care</a>
        </div>
    </div>
    <div class="col-md-9">
      <div class="srch-div listing">
        <form class="form-inline">
          <input class="form-control mr-sm-2" type="search" placeholder="Search for anything, anywhere in india" aria-label="Search">
          <button class="btn srch-btn"><i class="fa fa-search srch-icon" aria-hidden="true"></i></button>
        </form>
      </div>
      <div class="card-divs">
        <div class="row">
          <div class="col-md-1"></div>
          <div class="col-md-10">
            <p class="head-tab">Popular Services</p>
            <div class="row">
              <%  result.forEach(function(x){ %>
              <div class="col-md-6">
                <div class="card-div">
                  <div class="left-div">
                    <p class="text-heading"><%= x.name %></p>
                    <p class="sub-head">order-online</p>
                    <p class="sub-head">book table</p>
                    <p class="sub-head">tranding</p>
                    <p class="sub-head">more</p>
                  </div>
                  <div class="right-div">
                  <img class="crd-img" src="<%= x.img_url %>" alt="Card image cap">
                  </div>
                </div>
              </div>
              <% }) %>
            </div>
          </div>
          <div class="col-md-1"></div>
        </div>
      </div>
    </div>
  </div>