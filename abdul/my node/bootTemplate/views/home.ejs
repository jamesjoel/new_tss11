<h1 class="text-danger">home page</h1>

<div class="row">
  <div class="col-md-8 offset-md-2">
    
    <table class="teble table-bordered">
      <tr>
        <th>Name</th>
        <th>Age</th>
        <th>City</th>
      </tr>
      <% allData.forEach(function(data, i){ %>
